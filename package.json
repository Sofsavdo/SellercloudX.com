    "build": "npm run build:client && npm run build:server && node postbuild.js",
    "build:verify": "node -e \"const fs=require('fs'); const path=require('path'); if(!fs.existsSync('dist/public')){console.error('âŒ dist/public not found'); process.exit(1);} if(!fs.existsSync('dist/public/index.html')){console.error('âŒ dist/public/index.html not found'); process.exit(1);} if(!fs.existsSync('dist/index.js')){console.error('âŒ dist/index.js not found'); process.exit(1);} console.log('âœ… Build verification passed'); console.log('ðŸ“‚ dist/public contents:'); fs.readdirSync('dist/public').forEach(f=>console.log('  -',f));\"",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "seed": "cross-env NODE_ENV=development tsx server/seedData.ts",
    "deploy": "npm run build && npm start",
    "build:client": "npx vite build --config vite.config.ts",
    "build:server": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist --keep-names --target=node20",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "test": "cross-env NODE_ENV=test vitest run --dir server/__tests__ --reporter=verbose",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "production:setup": "npm run db:push && npm run build",
    "production:start": "npm run production:setup && npm start"
  },
